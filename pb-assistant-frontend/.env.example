# Expo public env vars used by the login screen. Copy to .env and replace with real values.
EXPO_PUBLIC_GOOGLE_EXPO_CLIENT_ID=
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=
EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID=
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=

# Set your official WeChat app ID to enable real WeChat login.
EXPO_PUBLIC_WECHAT_APP_ID=
# Optional universal link needed for iOS builds.
EXPO_PUBLIC_WECHAT_UNIVERSAL_LINK=

# Leave this true for local development with mocked WeChat codes. Set to false once the native SDK is wired up.
EXPO_PUBLIC_ENABLE_WECHAT_MOCK=true
